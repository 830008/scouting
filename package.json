{
  "name": "@isa2025/monorepo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names api,app -c blue,green \"pnpm api:dev\" \"pnpm app:dev\"",
